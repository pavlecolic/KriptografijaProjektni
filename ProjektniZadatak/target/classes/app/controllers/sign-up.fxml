<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.SignUpController">
    <children>
        <TextField fx:id="usernameField" layoutX="244.0" layoutY="110.0" promptText="username" />
        <PasswordField fx:id="passwordField" layoutX="244.0" layoutY="147.0" promptText="password" />
        <Label alignment="CENTER" layoutX="205.0" layoutY="40.0" prefHeight="42.0" prefWidth="162.0" text="SignUp">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label layoutX="161.0" layoutY="114.0" text="Username:" />
        <Label layoutX="163.0" layoutY="142.0" text="Password:" />
        <Label layoutX="106.0" layoutY="291.0" text="Already a member?" />
        <Button fx:id="loginButton" layoutX="121.0" layoutY="309.0" mnemonicParsing="false" onAction="#backToLogin" text="LogIn" />
        <Button fx:id="signUpButton" layoutX="366.0" layoutY="207.0" mnemonicParsing="false" onAction="#signUpClicked" text="SignUp" />
      <Label fx:id="signUpText" layoutX="69.0" layoutY="206.0" prefHeight="26.0" prefWidth="222.0" />
    </children>
</AnchorPane>
